{
  "manifest_version": 2,
  "name": "Nord Theme",
  "version": "1.1",
  "author": "@Ryan-da-Ghost & @QuiteAFancyEmerald",
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
      "default_icon": "logo.png"
  },
  "icons": {
       "120": "logo.png"
  },
      "browser_action": {
        "default_icon": "logo.png",
        "default_title": "Nord Website Settings",
        "default_popup": "popup.html"
    },
  "content_scripts": [{
      "matches": [
        "*://discord.com/*",
        "*://www.youtube.com/*",
        "*://www.deviantart.com/*",
        "*://monkeytype.com/*",
        "*://www.reddit.com/*"
      ],
      "js": ["javascript/urlCheck.js"]
    }],
  "web_accessible_resources": [
    "css/discord.css",
    "css/youtube.css",
    "css/deviantart.css",
    "css/monkeytype.css",
    "css/reddit.css"
  ]
  
}
